{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/results/[^/]+",
      "dest": "/index.html"
    },
    {
      "src": "/book/[^/]+",
      "dest": "/index.html"
    },
    {
      "src": "/nip3/?",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
